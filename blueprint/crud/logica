crear controladores
php artisan make:controller 'Admin\Institution\PescolarController'Z

define ruta
Route::resource('authorities', PescolarController::class)->only([ 'index']);

Livewire
php artisan make:livewire 'Admin/Institution/Pescolar/ListComponent'

crear los traits
Livewiere
mkdir -p '/home/nuser/code/pastoral/app/Http/Livewire/Admin/Institution/Pescolar/Traits'
touch '/home/nuser/code/pastoral/app/Http/Livewire/Admin/Institution/Pescolar/Traits/PescolarRules.php'

Model
mkdir -p '/home/nuser/code/pastoral/app/Models/app/Pescolar/Traits/Pescolar'
touch '/home/nuser/code/pastoral/app/Models/app/Pescolar/Traits/Pescolar/Relations.php'

composer dump-autoload

